{
  "version": 2,
  "builds": [
    {
      "src": "api/send-email.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/health.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/send-email",
      "dest": "/api/send-email.js",
      "methods": ["POST"]
    },
    {
      "src": "/api/health",
      "dest": "/api/health.js",
      "methods": ["GET"]
    }
  ]
} 